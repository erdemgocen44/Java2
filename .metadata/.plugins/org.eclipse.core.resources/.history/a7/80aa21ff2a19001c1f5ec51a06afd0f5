package swing1;

import java.awt.Color;

import javax.swing.ImageIcon;
import javax.swing.JFrame;

public class IlkFrame {
public static void main(String[] args) {
	
	JFrame ilkFrame=new JFrame();
	ilkFrame.setSize(400,400);
	ilkFrame.setTitle("Ýlk Frame");
	ilkFrame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	ImageIcon frameIcon=new ImageIcon("Images/1.jpg");
	ilkFrame.setIconImage(frameIcon.getImage());
	ilkFrame.getContentPane().setBackground(Color.LIGHT_GRAY);
	
	
	
	
	
	ilkFrame.setVisible(true);
	
	
	
	
}
}
